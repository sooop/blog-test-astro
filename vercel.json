{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "blog.soooprmx.com"
        }
      ],
      "destination": "https://www.soooprmx.com/blog/",
      "statusCode": 301
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "blog.soooprmx.com"
        }
      ],
      "destination": "https://www.soooprmx.com/blog/:path*",
      "statusCode": 301
    }
  ]
}
